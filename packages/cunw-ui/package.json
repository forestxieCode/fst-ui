  "scripts": {
    "dev": "npm run dev:umd & npm run dev:es & npm run dev:unpkg",
    "dev:umd": "rollup -c -w --format umd --file dist/cunw-ui.umd.js",
    "dev:es": "rollup -c -w --format es --file dist/cunw-ui.esm.js",
    "dev:unpkg": "rollup -c -w --format iife --file dist/cunw-ui.min.js",
